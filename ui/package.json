{
  "name": "mmx-node-web-gui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "mkdir -p dist && rm -rf dist/* || true",
    "lint": "bunx --bun eslint  ./ --fix",
    "format": "bunx --bun prettier --write \"**/*.{js,vue,scss,html,md,json}\" --list-different --ignore-path .gitignore --ignore-path .prettierignore",
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "dev:explorer": "bunx --bun vite --config vite.config.explorer.js",
    "build:explorer": "bunx --bun vite build --config vite.config.explorer.js",
    "preview:explorer": "bunx --bun vite preview --config vite.config.explorer.js",
    "dev:offline": "bunx --bun vite --config vite.config.offline.js",
    "build:offline": "bunx --bun vite build --config vite.config.offline.js",
    "preview:offline": "bunx --bun vite preview --config vite.config.offline.js",
    "build:all": "bun run build && bun run build:explorer",
    "build:android": "bun run build:explorer && bunx cap sync && bunx capacitor-assets generate --android && bunx cap build android",
    "dev:wxt": "bunx --bun wxt",
    "dev:wxt:firefox": "bunx --bun wxt -b firefox",
    "build:wxt": "bunx --bun wxt build",
    "build:wxt:firefox": "bunx --bun wxt build -b firefox",
    "zip:wxt": "bunx --bun wxt zip",
    "zip:wxt:firefox": "bunx --bun wxt zip -b firefox",
    "postinstall": "bunx --bun wxt prepare"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@fontsource-variable/roboto-flex": "^5.2.5",
    "@fontsource-variable/roboto-mono": "^5.2.5",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@mdi/js": "^7.4.47",
    "@metamask/browser-passworder": "^6.0.0",
    "@noble/hashes": "^1.8.0",
    "@noble/secp256k1": "^2.2.3",
    "@quasar/extras": "^1.17.0",
    "@scure/base": "^1.2.5",
    "@scure/bip39": "^1.6.0",
    "@tanstack/vue-query": "^5.77.1",
    "@tanstack/vue-query-devtools": "^5.77.1",
    "@vueuse/components": "^13.2.0",
    "@vueuse/core": "^13.2.0",
    "@vueuse/integrations": "^13.2.0",
    "@wxt-dev/storage": "^1.1.1",
    "animate.css": "^4.1.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "echarts": "^5.6.0",
    "fflate": "^0.8.2",
    "highlight.js": "^11.11.1",
    "json-bigint": "^1.0.0",
    "pinia": "^3.0.2",
    "qrcode": "^1.5.4",
    "quasar": "^2.18.1",
    "vue": "^3.5.14",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.4",
    "vue-router": "^4.5.1",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.2.0",
    "@eslint/compat": "^1.2.9",
    "@quasar/vite-plugin": "^1.9.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "bun": "1.1.24",
    "bunx": "^0.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.89.0",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-generate-file": "^0.3.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-singlefile": "^2.2.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.4",
    "wxt": "^0.20.6"
  },
  "packageManager": "pnpm@9.15.5"
}
